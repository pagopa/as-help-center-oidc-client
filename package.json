{
  "name": "as-help-center-oidc-client",
  "version": "1.22.0",
  "description": "OIDC client to handle CAC help center authentication",
  "main": "dist/src/app.js",
  "scripts": {
    "test": "jest",
    "test:silent": "jest --silent",
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.json",
    "package:lambda": "rm -f cac-oidc-client-lambda.zip && cd dist && zip -r ../cac-oidc-client-lambda.zip . && cd .. && zip -ur cac-oidc-client-lambda.zip node_modules package.json",
    "openapi:generate": "npm run build && node dist/scripts/generate-openapi.js",
    "openapi:generate-infra": "npm run openapi:generate && node dist/scripts/sync-infra-openapi.js",
    "start": "node dist/server.js",
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write '**/*.{ts,js,mjs,json,yml,yaml,md}'",
    "prepare": "node .husky/install.mjs"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@aws-sdk/client-ssm": "^3.936.0",
    "@codegenie/serverless-express": "^4.17.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "openid-client": "^5.6.4",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/aws-lambda": "^8.10.158",
    "@types/express": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.16.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.5.3",
    "semantic-release": "^25.0.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --quiet --fix"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pagopa/as-help-center-oidc-client.git"
  },
  "author": "PagoPa (https://github.com/pagopa)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pagopa/as-help-center-oidc-client/issues"
  },
  "homepage": "https://github.com/pagopa/as-help-center-oidc-client#readme"
}
